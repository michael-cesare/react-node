{
  "compilerOptions": {
    "moduleResolution": "node",
    "outDir": "./built/",
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "typeRoots": ["node_modules/@types"]
  },
  "include": ["src"],
  "exclude": ["node_modues"]
}